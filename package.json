{
  "name": "rebilly-js-sdk",
  "version": "0.0.20",
  "description": "JavaScript SDK for Rebilly.com",
  "main": "./dist/rebilly-js-sdk.js",
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "build:prod": "npm run clean && webpack --env=prod",
    "build:dev": "npm run clean && webpack --env=dev",
    "prepublish": "npm run build:prod",
    "unit": "mocha --compilers js:babel-core/register --require babel-polyfill --colors ./test/unit/specs/*.spec.js",
    "unit:watch": "mocha --watch --compilers js:babel-core/register --require babel-polyfill --colors ./test/unit/specs/*.spec.js",
    "integration": "mocha --timeout 10000 --compilers js:babel-core/register --require babel-polyfill --colors ./test/integration/specs/**/*.spec.js",
    "integration:watch": "mocha --watch --timeout 10000 --compilers js:babel-core/register --require babel-polyfill --colors ./test/integration/specs/**/*.spec.js",
    "integration:watch:only": "mocha --watch --timeout 10000 --compilers js:babel-core/register --require babel-polyfill --colors",
    "test": "npm run unit && npm run integration",
    "coverage": "npm run clean && cross-env NODE_ENV=test nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Teknologica/js-sdk.git"
  },
  "keywords": [
    "js",
    "sdk",
    "rebilly"
  ],
  "files": [
    "index.js",
    "version.js",
    "dist"
  ],
  "author": "Andrei Moldoveanu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Teknologica/js-sdk/issues"
  },
  "homepage": "https://github.com/Teknologica/js-sdk#readme",
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "https://registry.npmjs.org/babel-register/-/babel-register-6.24.1.tgz",
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "dotenv": "^4.0.0",
    "faker": "^4.1.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.0.3",
    "rimraf": "^2.6.1",
    "webpack": "^2.5.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
